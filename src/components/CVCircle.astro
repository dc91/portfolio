---
const { fillColor = "#000" } = Astro.props;
const { fillOpacity = "1" } = Astro.props;
const { strokeColor = "#000" } = Astro.props;
const { strokeWidth = "3" } = Astro.props;
---

<svg height="20" width="20" xmlns="http://www.w3.org/2000/svg">
    <circle
        r="8"
        cx="10"
        cy="10"
        fill={fillColor}
        stroke={strokeColor}
        stroke-width={strokeWidth}
        opacity={fillOpacity}></circle>
</svg>
